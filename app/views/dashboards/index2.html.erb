<h1>Dashboard</h1>
<% if notice && notice != '' %>
	<p id="notice"><%= notice %></p>
<% end %>

<div class="edit-dashboards-button" >
	<% if Dashboard.count > 0 %>
	   <%=link_to Dashboard.first.name, edit_dashboard_path(Dashboard.first) %>
	<% end %>
</div>

<%# consumable_objects_buttons %>


<%# consumed_units_table %>



<%# rescue time charts %>
<div class="rescue-time-charts">
	<% RescueTimeChart.all.each do |rct| %>
	    <% if rct.show %>
	        <%= raw(process_rescue_time_script(rct)) %>
	    <% end %>
	<% end %>
</div>

